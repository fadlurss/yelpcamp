<% include ../partials/admin/header %>
<div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Pasien Tables</h1>
            <br><br>
            </div>
           
            <!-- /.col-lg-12 -->
        </div>
        <div class="help-block">
            <a href="/pasien/new" class="btn btn-info">Tambah pasien baru</a>
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Pasien Tables
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover" id="datatable">
                            <thead>
                                <tr>
                                    <th>Nama</th>
                                    <th>Nomor BPJS</th>
                                    <th>Tanggal Lahir</th>
                                    <th>Umur</th>
                                    <th>Nomor HP</th>
                                    <th>Nama Ibu Kandung</th>
                                    <th>Alamat</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data_pasien.forEach(function(hasil){ %>
                              <tr>
                                  <td><%= hasil.nama_lengkap %></td>
                                  <td><%= hasil.nomor_bpjs %></td>
                                  <td><%= moment(hasil.tanggal_lahir).format('LL') %></td>
                                  <td></td>
                                  <td><%= hasil.nomor_hp %></td>
                                  <td><%= hasil.nama_ibu_kandung%></td>
                                  <td><%= hasil.alamat%></td>
                              </tr>
                              <% }); %>
                            </tbody>
                        </table>
                       
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>     
</div>
    <!-- /#page-wrapper -->
<% include ../partials/admin/footer %>